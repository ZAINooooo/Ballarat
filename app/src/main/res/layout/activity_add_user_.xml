<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">



    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:layout_marginBottom="20dp"
        android:layout_centerHorizontal="true"
        android:src="@drawable/logo" />


    <Toolbar
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/imageView"
        android:minHeight="?attr/actionBarSize"
        android:background="#0f75bc"
        android:layout_marginBottom="20dp">


        <ImageView
            android:id="@+id/imageViewf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="28dp"
            android:layout_marginBottom="20dp"
            android:layout_centerHorizontal="true"
            android:src="@drawable/xa" />


        <TextView
            android:id="@+id/m1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/mons"
            android:text="User Setting"
            android:textColor="#ffffff"
            android:layout_gravity="center"
            android:paddingTop="5dp"
            android:textSize="22dp" />

    </Toolbar>


    <TextView
        android:id="@+id/m2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/mons"
        android:text="Mark Wilson"
        android:layout_below="@id/toolbar"
        android:layout_centerHorizontal="true"
        android:textColor="#000000"
        android:layout_gravity="center"
        android:paddingTop="5dp"
        android:textSize="22dp" />


    <View
        android:id="@+id/mkj"
        android:layout_width="fill_parent"
        android:layout_height="3px"
        android:layout_below="@id/m2"
        android:layout_marginTop="30dp"
        android:background="@android:color/darker_gray" />




    <TextView
        android:id="@+id/m3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/mons"
        android:text="Blogs"
        android:layout_below="@id/mkj"
        android:layout_margin="30dp"
        android:textColor="#000000"
        android:layout_gravity="center"
        android:textSize="22dp" />




    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/mkj"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="45dp"
        android:paddingRight="30dp"
        android:src="@drawable/csdaa" />




    <View
        android:id="@+id/mkj2"
        android:layout_width="fill_parent"
        android:layout_height="3px"
        android:layout_below="@id/m3"
        android:background="@android:color/darker_gray" />

















    <TextView
        android:id="@+id/m33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/mons"
        android:text="Excel File"
        android:layout_below="@id/mkj2"
        android:layout_margin="30dp"
        android:textColor="#000000"
        android:layout_gravity="center"
        android:textSize="22dp" />



    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/mkj2"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="45dp"
        android:paddingRight="30dp"
        android:src="@drawable/csdaa" />



    <View
        android:id="@+id/mkj22"
        android:layout_width="fill_parent"
        android:layout_height="3px"
        android:layout_below="@id/m33"
        android:background="@android:color/darker_gray" />





    <TextView
        android:id="@+id/m3333"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/mons"
        android:text=""
        android:layout_below="@id/mkj22"
        android:layout_margin="30dp"
        android:textColor="#000000"
        android:layout_gravity="center"
        android:textSize="22dp" />

    <View
        android:id="@+id/mkj222"
        android:layout_width="fill_parent"
        android:layout_height="3px"
        android:layout_below="@id/m3333"
        android:background="@android:color/darker_gray" />








    <TextView
        android:id="@+id/m33334"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/mons"
        android:text=""
        android:layout_below="@id/mkj222"
        android:layout_margin="30dp"
        android:textColor="#000000"
        android:layout_gravity="center"
        android:textSize="22dp" />

    <View
        android:id="@+id/mkj2222"
        android:layout_width="fill_parent"
        android:layout_height="3px"
        android:layout_below="@id/m33334"
        android:background="@android:color/darker_gray" />

</RelativeLayout>